function Trig_Start_Static_Waves_Actions takes nothing returns nothing
    call MultiboardSetItemValueBJ(GetLastCreatedMultiboard(), 2, 14, ( ( "|cffFDD017" + I2S(udg_Integer_Array_WaveTimer[1]) ) + ( " Seconds|r" ) ))
    if ( Trig_Start_Static_Waves_Func002C() ) then
        set udg_Integer_SpawnMax=60
    else
        set udg_Integer_SpawnMax=30
    endif
    set udg_Integer_SpawnCount=0
    call StopSoundBJ(GetLastPlayedSound(), false)
    set udg_Integer_WaveNumber=( udg_Integer_WaveNumber + 1 )
    if ( Trig_Start_Static_Waves_Func006C() ) then
        call ForForce(udg_PlayerGroup_PlyGrpArray[1], function Trig_Start_Static_Waves_Func006Func001A)
    else
    endif
    call MultiboardSetItemValueBJ(GetLastCreatedMultiboard(), 1, 14, "TRIGSTR_305")
    if ( Trig_Start_Static_Waves_Func008C() ) then
        set udg_Real_CreepHealthMultiplier=GetRandomReal(udg_Real_Array_ChaosHealthPerLow[udg_Integer_Difficulty], udg_Real_Array_ChaosHealthPerHigh[udg_Integer_Difficulty])
        set udg_String_Array_ChaosMultiplier[1]=R2S(udg_Real_CreepHealthMultiplier)
    else
    endif
    // Normal Creeps
    if ( Trig_Start_Static_Waves_Func010C() ) then
