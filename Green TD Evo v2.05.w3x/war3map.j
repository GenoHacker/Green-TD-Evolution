//===========================================================================
// Trigger: Timer
//
// Trigger that controls the timer on the multiboard, it counts down 10 seconds, with this trigger updating the mb each second until it hits 0 when it will eventually indicate that a wave has spawned.
//===========================================================================
function Trig_Timer_Func002C takes nothing returns boolean
    if ( not ( udg_Integer_Array_WaveTimer[1] >= 2 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Timer_Func003C takes nothing returns boolean
    if ( not ( udg_Integer_Array_WaveTimer[1] <= 1 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Timer_Func004Func001C takes nothing returns boolean
    if ( not ( udg_Integer_BlitzMode == 1 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Timer_Func004C takes nothing returns boolean
    if ( not ( udg_Integer_Array_WaveTimer[1] == 0 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Timer_Actions takes nothing returns nothing
    set udg_Integer_Array_WaveTimer[1]=( udg_Integer_Array_WaveTimer[1] - 1 )
    if ( Trig_Timer_Func002C() ) then
        call MultiboardSetItemValueBJ(GetLastCreatedMultiboard(), 2, 14, ( ( "|cffFDD017" + I2S(udg_Integer_Array_WaveTimer[1]) ) + ( " Seconds|r" ) ))
    else
    endif
    if ( Trig_Timer_Func003C() ) then
        call MultiboardSetItemValueBJ(GetLastCreatedMultiboard(), 2, 14, ( ( "|cffFDD017" + I2S(udg_Integer_Array_WaveTimer[1]) ) + ( " Second|r" ) ))
    else
    endif
    if ( Trig_Timer_Func004C() ) then
        if ( Trig_Timer_Func004Func001C() ) then
            set udg_Integer_Array_WaveTimer[1]=R2I(udg_Real_SpawnTime)
            call TriggerExecute(gg_trg_Blitz_Spawning)
        else
            call MultiboardSetItemValueBJ(GetLastCreatedMultiboard(), 2, 14, "TRIGSTR_761")
            call DisableTrigger(GetTriggeringTrigger())
