//===========================================================================
// Trigger: Player Leave Gold Split
//
// When a player leaves this triggers splits there current gold pool equally between all remaining players. It also displays an ingame message and modifies the multiboard to reflect the player change.
//===========================================================================
function Trig_Player_Leave_Gold_Split_Func003002 takes nothing returns nothing
    call RemoveUnit(GetEnumUnit())
endfunction

function Trig_Player_Leave_Gold_Split_Func009Func001C takes nothing returns boolean
    if ( not ( GetPlayerSlotState(GetEnumPlayer()) == PLAYER_SLOT_STATE_PLAYING ) ) then
        return false
    endif
    return true
endfunction

function Trig_Player_Leave_Gold_Split_Func009A takes nothing returns nothing
    if ( Trig_Player_Leave_Gold_Split_Func009Func001C() ) then
        call DisplayTimedTextToForce(GetForceOfPlayer(GetEnumPlayer()), udg_Real_Array_MessageTime[GetConvertedPlayerId(GetEnumPlayer())], ( udg_String_Array_PlayerNames[GetConvertedPlayerId(GetTriggerPlayer())] + ( GetPlayerName(GetTriggerPlayer()) + ( "|cff00FF00, Has left the game. You have been given|r |cffFDD017" + ( I2S(( udg_Integer_PlayerLeaveGold / udg_Integer_Players )) + " Gold.|r" ) ) ) ))
        call AdjustPlayerStateBJ(( udg_Integer_PlayerLeaveGold / udg_Integer_Players ), GetEnumPlayer(), PLAYER_STATE_RESOURCE_GOLD)
    else
    endif
endfunction

function Trig_Player_Leave_Gold_Split_Func011C takes nothing returns boolean
    if ( not ( GetTriggerPlayer() == GetOwningPlayer(gg_unit_n00C_0019) ) ) then
        return false
    endif
    return true
endfunction

function Trig_Player_Leave_Gold_Split_Actions takes nothing returns nothing
    set udg_Integer_PlayerLeaveGold=GetPlayerState(GetTriggerPlayer(), PLAYER_STATE_RESOURCE_GOLD)
    call ForceRemovePlayerSimple(GetTriggerPlayer(), udg_PlayerGroup_PlyGrpArray[1])
    call ForGroupBJ(GetUnitsInRectOfPlayer(GetPlayableMapRect(), GetTriggerPlayer()), function Trig_Player_Leave_Gold_Split_Func003002)
    call MultiboardSetItemValueBJ(GetLastCreatedMultiboard(), 1, ( GetConvertedPlayerId(GetTriggerPlayer()) + 1 ), ( udg_String_Array_MultiBoardColours[GetConvertedPlayerId(GetTriggerPlayer())] + "Left the Game|r" ))
    call MultiboardSetItemValueBJ(GetLastCreatedMultiboard(), 2, ( GetConvertedPlayerId(GetTriggerPlayer()) + 1 ), ( "|cffFDD0170" ))
    call MultiboardSetItemValueBJ(GetLastCreatedMultiboard(), 3, ( GetConvertedPlayerId(GetTriggerPlayer()) + 1 ), ( "|cffFDD0170" ))
    set udg_Integer_Array_Kills[GetConvertedPlayerId(GetTriggerPlayer())]=0
    set udg_Integer_Players=( udg_Integer_Players - 1 )
    call ForForce(udg_PlayerGroup_PlyGrpArray[1], function Trig_Player_Leave_Gold_Split_Func009A)
    call SetPlayerStateBJ(GetTriggerPlayer(), PLAYER_STATE_RESOURCE_GOLD, 0)
    if ( Trig_Player_Leave_Gold_Split_Func011C() ) then
        call SetUnitOwner(gg_unit_n00C_0019, Player(PLAYER_NEUTRAL_PASSIVE), true)
        call EnableTrigger(gg_trg_Game_Setup_Ownership)
    else
    endif
endfunction

//===========================================================================
function InitTrig_Player_Leave_Gold_Split takes nothing returns nothing
    set gg_trg_Player_Leave_Gold_Split=CreateTrigger()
    call TriggerRegisterPlayerEventLeave(gg_trg_Player_Leave_Gold_Split, Player(0))
    call TriggerRegisterPlayerEventLeave(gg_trg_Player_Leave_Gold_Split, Player(1))
    call TriggerRegisterPlayerEventLeave(gg_trg_Player_Leave_Gold_Split, Player(2))
    call TriggerRegisterPlayerEventLeave(gg_trg_Player_Leave_Gold_Split, Player(3))
