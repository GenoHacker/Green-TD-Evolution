//===========================================================================
function Trig_Generate_Next_Units_Actions takes nothing returns nothing
    set bj_forLoopAIndex=0
    set bj_forLoopAIndexEnd=4
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call RemoveUnit(udg_Unit_Array_NextUnits[GetForLoopIndexA()])
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call CreateNUnitsAtLoc(1, udg_UnitType_Array_RWSpawnUnit[udg_Integer_WaveNumber], Player(PLAYER_NEUTRAL_PASSIVE), GetRectCenter(udg_Region_Array_RandomWaves[0]), bj_UNIT_FACING)
    call BlzSetUnitName(GetLastCreatedUnit(), ( "Wave " + I2S(udg_Integer_WaveNumber) ))
    set udg_Unit_Array_NextUnits[0]=GetLastCreatedUnit()
    call BlzSetUnitMaxHP(GetLastCreatedUnit(), R2I(udg_Real_Array_CreepHealthWaves[udg_Integer_WaveNumber]))
    call SetUnitLifePercentBJ(GetLastCreatedUnit(), 100)
    call BlzSetUnitArmor(GetLastCreatedUnit(), udg_Real_Array_RWArmourTotal[udg_Integer_WaveNumber])
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=4
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call CreateNUnitsAtLoc(1, udg_UnitType_Array_RWSpawnUnit[( udg_Integer_WaveNumber + GetForLoopIndexA() )], Player(PLAYER_NEUTRAL_PASSIVE), GetRectCenter(udg_Region_Array_RandomWaves[GetForLoopIndexA()]), bj_UNIT_FACING)
        call BlzSetUnitName(GetLastCreatedUnit(), ( "Wave " + I2S(( udg_Integer_WaveNumber + GetForLoopIndexA() )) ))
        set udg_Unit_Array_NextUnits[GetForLoopIndexA()]=GetLastCreatedUnit()
        call BlzSetUnitMaxHP(GetLastCreatedUnit(), R2I(udg_Real_Array_CreepHealthWaves[( udg_Integer_WaveNumber + GetForLoopIndexA() )]))
        call SetUnitLifePercentBJ(GetLastCreatedUnit(), 100)
        call BlzSetUnitArmor(GetLastCreatedUnit(), udg_Real_Array_RWArmourTotal[( udg_Integer_WaveNumber + GetForLoopIndexA() )])
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
endfunction

