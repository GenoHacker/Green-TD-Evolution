function Trig_Choose_Mode_Gamemodes_Actions takes nothing returns nothing
    call DisableTrigger(gg_trg_Default_Gamemode)
    // ------------------------------
    // Difficulty
    // ------------------------------
    // Easy Difficulty
    // ------------------------------
    if ( Trig_Choose_Mode_Gamemodes_Func008C() ) then
        set udg_Integer_Difficulty=1
    else
    endif
    // ------------------------------
    // Normal Difficulty
    // ------------------------------
    if ( Trig_Choose_Mode_Gamemodes_Func012C() ) then
        set udg_Integer_Difficulty=2
    else
    endif
    // ------------------------------
    // Hard Difficulty
    // ------------------------------
    if ( Trig_Choose_Mode_Gamemodes_Func016C() ) then
        set udg_Integer_Difficulty=3
    else
    endif
    // ------------------------------
    // Very Hard Difficulty
    // ------------------------------
    if ( Trig_Choose_Mode_Gamemodes_Func020C() ) then
        set udg_Integer_Difficulty=4
    else
    endif
    // ------------------------------
    // Chaos Difficulty
    // ------------------------------
    if ( Trig_Choose_Mode_Gamemodes_Func024C() ) then
        set udg_Integer_ChaosModeOn=1
        set udg_Integer_ChaosMode=1
        set udg_Integer_Difficulty=1
    else
        if ( Trig_Choose_Mode_Gamemodes_Func024Func001C() ) then
            set udg_Integer_ChaosModeOn=1
            set udg_Integer_ChaosMode=2
            set udg_Integer_Difficulty=2
        else
            if ( Trig_Choose_Mode_Gamemodes_Func024Func001Func001C() ) then
                set udg_Integer_ChaosModeOn=1
                set udg_Integer_ChaosMode=3
                set udg_Integer_Difficulty=3
            else
                if ( Trig_Choose_Mode_Gamemodes_Func024Func001Func001Func001C() ) then
                    set udg_Integer_ChaosModeOn=1
                    set udg_Integer_ChaosMode=4
                    set udg_Integer_Difficulty=4
                else
                    if ( Trig_Choose_Mode_Gamemodes_Func024Func001Func001Func001Func001C() ) then
                        set udg_Integer_ChaosModeOn=1
                        set udg_Integer_ChaosMode=5
                        set udg_Integer_Difficulty=5
                    else
                    endif
                endif
            endif
        endif
    endif
    // ------------------------------
    // Game Modes
    // ------------------------------
    // Endless Mode
    if ( Trig_Choose_Mode_Gamemodes_Func029C() ) then
        if ( Trig_Choose_Mode_Gamemodes_Func029Func001C() ) then
            call BlzSetAbilityTooltip(GetSpellAbilityId(), "|c0077ff77[Q]|r |c0000ff00Activate Endless Mode", 1)
            call BlzSetAbilityExtendedTooltip(GetSpellAbilityId(), "|c0000ff00Status = |c00FF0000Deactivated", 1)
            set udg_Integer_EndlessMode=0
        else
            call BlzSetAbilityTooltip(GetSpellAbilityId(), "|c0077ff77[Q]|r |c0000ff00Deactivate Endless Mode", 1)
            call BlzSetAbilityExtendedTooltip(GetSpellAbilityId(), "|c0000ff00Status = |c0077ff77Activated", 1)
            set udg_Integer_EndlessMode=1
        endif
    else
    endif
    // Gold Rush Mode
    if ( Trig_Choose_Mode_Gamemodes_Func031C() ) then
        if ( Trig_Choose_Mode_Gamemodes_Func031Func001C() ) then
            call BlzSetAbilityTooltip(GetSpellAbilityId(), "|c0077ff77[W]|r |c0000ff00Activate Gold Rush Mode", 1)
            call BlzSetAbilityExtendedTooltip(GetSpellAbilityId(), "|c0000ff00Status = |c00FF0000Deactivated", 1)
            set udg_Integer_GoldRush=0
        else
            call BlzSetAbilityTooltip(GetSpellAbilityId(), "|c0077ff77[W]|r |c0000ff00Deactivate Gold Rush Mode", 1)
            call BlzSetAbilityExtendedTooltip(GetSpellAbilityId(), "|c0000ff00Status = |c0077ff77Activated", 1)
            set udg_Integer_GoldRush=1
        endif
    else
    endif
    // Swarm Mode
    if ( Trig_Choose_Mode_Gamemodes_Func033C() ) then
        if ( Trig_Choose_Mode_Gamemodes_Func033Func001C() ) then
            call BlzSetAbilityTooltip(GetSpellAbilityId(), "|c0077ff77[E]|r |c0000ff00Activate Swarm Mode", 1)
            call BlzSetAbilityExtendedTooltip(GetSpellAbilityId(), "|c0000ff00Status = |c00FF0000Deactivated", 1)
            set udg_Integer_SwarmMode=0
        else
            call BlzSetAbilityTooltip(GetSpellAbilityId(), "|c0077ff77[E]|r |c0000ff00Deactivate Swarm Mode", 1)
            call BlzSetAbilityExtendedTooltip(GetSpellAbilityId(), "|c0000ff00Status = |c0077ff77Activated", 1)
            set udg_Integer_SwarmMode=1
        endif
    else
    endif
    // Random Waves
    if ( Trig_Choose_Mode_Gamemodes_Func035C() ) then
