function Trig_Blood_Trap_Conditions takes nothing returns boolean
    if ( not ( GetSpellAbilityId() == 'A023' ) ) then
        return false
    endif
    if ( not ( GetUnitTypeId(GetTriggerUnit()) == 'n01H' ) ) then
        return false
    endif
    return true
endfunction

function Trig_Blood_Trap_Func004Func005C takes nothing returns boolean
    if ( not ( GetUnitAbilityLevelSwapped('A008', GetTriggerUnit()) >= 10 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Blood_Trap_Func004C takes nothing returns boolean
    if ( not ( GetPlayerState(GetOwningPlayer(GetTriggerUnit()), PLAYER_STATE_RESOURCE_GOLD) < GetUnitUserData(GetTriggerUnit()) ) ) then
        return false
    endif
    return true
endfunction

function Trig_Blood_Trap_Actions takes nothing returns nothing
    call SetUnitUserData(GetTriggerUnit(), udg_Integer_Array_TrapGoldCost[GetUnitAbilityLevelSwapped('A008', GetTriggerUnit())])
    if ( Trig_Blood_Trap_Func004C() ) then
        call CreateTextTagUnitBJ(( "You Need " + ( I2S(GetUnitUserData(GetTriggerUnit())) + " To Upgrade" ) ), GetSpellAbilityUnit(), 0, 10.00, 0.00, 100, 0.00, 0)
    else
        call CreateTextTagUnitBJ("TRIGSTR_9110", GetSpellAbilityUnit(), 0, 10.00, 0.00, 100, 0.00, 0)
        call IncUnitAbilityLevelSwapped('A008', GetTriggerUnit())
        call BlzSetUnitName(GetTriggerUnit(), ( "Blood Trap " + I2S(GetUnitAbilityLevelSwapped('A008', GetTriggerUnit())) ))
        call SetPlayerStateBJ(GetOwningPlayer(GetTriggerUnit()), PLAYER_STATE_RESOURCE_GOLD, ( GetPlayerState(GetOwningPlayer(GetTriggerUnit()), PLAYER_STATE_RESOURCE_GOLD) - GetUnitUserData(GetTriggerUnit()) ))
        if ( Trig_Blood_Trap_Func004Func005C() ) then
            call UnitRemoveAbilityBJ('A023', GetTriggerUnit())
        else
        endif
    endif
    call SetTextTagVelocityBJ(GetLastCreatedTextTag(), 40.00, 90)
    call SetTextTagPermanentBJ(GetLastCreatedTextTag(), false)
    call SetTextTagFadepointBJ(GetLastCreatedTextTag(), 2.00)
    call SetTextTagLifespanBJ(GetLastCreatedTextTag(), 3.00)
    call ShowTextTagForceBJ(false, GetLastCreatedTextTag(), GetPlayersAll())
    call ShowTextTagForceBJ(true, GetLastCreatedTextTag(), GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())))
endfunction

//===========================================================================
function InitTrig_Blood_Trap takes nothing returns nothing
    set gg_trg_Blood_Trap=CreateTrigger()
    call TriggerRegisterAnyUnitEventBJ(gg_trg_Blood_Trap, EVENT_PLAYER_UNIT_SPELL_CAST)
    call TriggerAddCondition(gg_trg_Blood_Trap, Condition(function Trig_Blood_Trap_Conditions))
    call TriggerAddAction(gg_trg_Blood_Trap, function Trig_Blood_Trap_Actions)
endfunction

