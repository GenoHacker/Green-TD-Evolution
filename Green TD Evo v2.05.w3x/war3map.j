function Trig_Random_Game_Modes_and_Difficulty_Actions takes nothing returns nothing
    if ( Trig_Random_Game_Modes_and_Difficulty_Func001C() ) then
        set udg_Integer_IsRandomModes=1
        // ------------------------------
        // Difficulty Settings
        // ------------------------------
        set udg_Integer_RandomDifficulty=GetRandomInt(1, 9)
        if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func007C() ) then
            // Easy 
            set udg_Integer_Difficulty=1
            call EnableTrigger(gg_trg_Income)
            set udg_Integer_SellPercent=100
            set udg_Real_CreepHealthMultiplier=udg_Real_Array_Handicap[udg_Integer_Difficulty]
        else
            if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func007Func007C() ) then
                // Easy - Chaos Mode
                set udg_Integer_ChaosModeOn=1
                set udg_Integer_ChaosMode=1
                set udg_Integer_Difficulty=1
                set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 2 )
                call EnableTrigger(gg_trg_Income)
                set udg_Integer_SellPercent=100
                set udg_Real_CreepHealthMultiplier=udg_Real_Array_Handicap[udg_Integer_Difficulty]
            else
                if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func007Func007Func009C() ) then
                    // Normal
                    set udg_Integer_Difficulty=2
                    set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 2 )
                    call EnableTrigger(gg_trg_Income)
                    set udg_Integer_SellPercent=80
                    set udg_Real_CreepHealthMultiplier=udg_Real_Array_Handicap[udg_Integer_Difficulty]
                else
                    if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func007Func007Func009Func007C() ) then
                        // Normal - Chaos Mode
                        set udg_Integer_ChaosModeOn=1
                        set udg_Integer_ChaosMode=2
                        set udg_Integer_Difficulty=2
                        set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 4 )
                        call EnableTrigger(gg_trg_Income)
                        set udg_Integer_SellPercent=80
                        set udg_Real_CreepHealthMultiplier=udg_Real_Array_Handicap[udg_Integer_Difficulty]
                    else
                        if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func007Func007Func009Func007Func009C() ) then
                            // Hard
                            set udg_Integer_Difficulty=3
                            set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 4 )
                            call EnableTrigger(gg_trg_Income)
                            set udg_Integer_SellPercent=60
                            set udg_Real_CreepHealthMultiplier=udg_Real_Array_Handicap[udg_Integer_Difficulty]
                        else
                            if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func007Func007Func009Func007Func009Func003C() ) then
                                // Hard - Chaos Mode
                                set udg_Integer_ChaosModeOn=1
                                set udg_Integer_ChaosMode=3
                                set udg_Integer_Difficulty=3
                                set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 6 )
                                call EnableTrigger(gg_trg_Income)
                                set udg_Integer_SellPercent=60
                                set udg_Real_CreepHealthMultiplier=udg_Real_Array_Handicap[udg_Integer_Difficulty]
                            else
                                if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func007Func007Func009Func007Func009Func003Func005C() ) then
                                    // Very Hard
                                    set udg_Integer_Difficulty=4
                                    set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 6 )
                                    call EnableTrigger(gg_trg_Income)
                                    set udg_Integer_SellPercent=40
                                    set udg_Real_CreepHealthMultiplier=udg_Real_Array_Handicap[udg_Integer_Difficulty]
                                else
                                    if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func007Func007Func009Func007Func009Func003Func005Func003C() ) then
                                        // Very Hard - Chaos Mode
                                        set udg_Integer_ChaosModeOn=1
                                        set udg_Integer_ChaosMode=4
                                        set udg_Integer_Difficulty=4
                                        set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 8 )
                                        call EnableTrigger(gg_trg_Income)
                                        set udg_Integer_SellPercent=40
                                        set udg_Real_CreepHealthMultiplier=udg_Real_Array_Handicap[udg_Integer_Difficulty]
                                    else
                                        if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func007Func007Func009Func007Func009Func003Func005Func003Func005C() ) then
                                            // Chaos Mode
                                            set udg_Integer_ChaosModeOn=1
                                            set udg_Integer_ChaosMode=5
                                            set udg_Integer_Difficulty=5
                                            set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 8 )
                                            call EnableTrigger(gg_trg_Income)
                                            set udg_Integer_SellPercent=20
                                            set udg_Real_CreepHealthMultiplier=udg_Real_Array_Handicap[udg_Integer_Difficulty]
                                        else
                                        endif
                                    endif
                                endif
                            endif
                        endif
                    endif
                endif
            endif
        endif
        // ------------------------------
        // Builders
        // ------------------------------
        call ForForce(udg_PlayerGroup_PlyGrpArray[1], function Trig_Random_Game_Modes_and_Difficulty_Func001Func011A)
        // ------------------------------
        // Game Modes
        // ------------------------------
        set bj_forLoopAIndex=1
        set bj_forLoopAIndexEnd=7
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            set udg_Integer_Array_RandomGameModes[GetForLoopIndexA()]=GetRandomInt(0, 1)
            set bj_forLoopAIndex=bj_forLoopAIndex + 1
        endloop
        // Sets 7 arrays in the variable to a random number between 0 and 1. 0 = Deactivated 1 = Activated
        // ------------------------------
        // Gold Rush
        // ------------------------------
        if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func020C() ) then
            set udg_Integer_GoldRush=1
            call EnableTrigger(gg_trg_Gold_Rush_Mode)
            call EnableTrigger(gg_trg_Gold_Rush_Bounty)
            set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 2 )
        else
        endif
        // ------------------------------
        // Swarm Mode
        // ------------------------------
        if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func024C() ) then
            set udg_Integer_SwarmMode=1
            if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func024Func002C() ) then
            else
                set udg_Integer_SpawnMax=60
            endif
            set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 2 )
        else
        endif
        // ------------------------------
        // Random Waves
        // ------------------------------
        if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func028C() ) then
            set udg_Integer_RWActivated=1
            call TriggerExecute(gg_trg_Set_Random_Wave_Variables)
            call TriggerExecute(gg_trg_Generate_Random_Waves)
            call TriggerExecute(gg_trg_Start_Random_Waves)
            call EnableTrigger(gg_trg_Random_Wave_Bounty)
            set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 2 )
        else
            call TriggerExecute(gg_trg_Start_Static_Waves)
            set udg_Integer_Array_WaveTimer[1]=R2I(udg_Real_SpawnTime)
        endif
        // ------------------------------
        // Endless Mode
        // ------------------------------
        if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func032C() ) then
            set udg_Integer_EndlessMode=1
            set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 2 )
        else
        endif
        // ------------------------------
        // Chaos Mode
        // Chaos Mode isnt strictly a game mode like the others as it is attachted directly to difficulty modes
        // ------------------------------
        if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func037C() ) then
            set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 2 )
        else
        endif
        // ------------------------------
        // Headhunter Mode
        // ------------------------------
        if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func041C() ) then
            set udg_Integer_HHActivated=1
            call ShowUnitShow(gg_unit_n00D_0042)
            call TriggerExecute(gg_trg_Set_Random_Wave_Variables)
            call EnableTrigger(gg_trg_Headhunter_Mode)
            call EnableTrigger(gg_trg_Headhunter_Reward)
            call EnableTrigger(gg_trg_Headhunter_Actives)
            set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 2 )
        else
        endif
        // ------------------------------
        // Speed Mode
        // ------------------------------
        if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func045C() ) then
            set udg_Integer_SpeedMode=1
            set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 2 )
        else
        endif
        // ------------------------------
        // Blitz Mode
        // ------------------------------
        if ( Trig_Random_Game_Modes_and_Difficulty_Func001Func049C() ) then
            set udg_Integer_BlitzMode=1
            call DisableTrigger(gg_trg_Run_Start_Waves)
            call TriggerExecute(gg_trg_Blitz_Mode)
            set udg_Integer_IncomeGold=( udg_Integer_IncomeGold + 2 )
            set udg_Integer_WaveNumber=0
        else
            call EnableTrigger(gg_trg_Run_Start_Waves)
            set udg_Integer_Array_WaveTimer[1]=R2I(udg_Real_SpawnTime)
        endif
        set udg_Integer_Gamemode=1
        set udg_Real_Array_MessageTime[1]=5.00
        call ShowUnitHide(gg_unit_n00C_0019)
        call ClearTextMessagesBJ(GetPlayersAll())
        call DisplayTimedTextToForce(GetPlayersAll(), udg_Real_Array_MessageTime[1], ( udg_String_Array_PlayerNames[GetConvertedPlayerId(GetOwningPlayer(gg_unit_n00C_0019))] + "|c0000ff00 Has chosen |c0077ff77Random Game Modes and Random Difficulty. |c0000ff00Good Luck." ))
        call EnableTrigger(gg_trg_Debug_Wave)
        call DisableTrigger(gg_trg_Turn_On_RedAfk_Ready)
    else
    endif
endfunction

