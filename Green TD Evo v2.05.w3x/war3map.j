    call DisplayTimedTextToForce(udg_PlayerGroup_PlyGrpArray[3], 120.00, ( ( "|c0000ff00If|r " + udg_String_Array_PlayerNames[1] ) + " |c0000ff00picks a game option but does not start the game then after|r |c0077ff77120 seconds|r|c0000ff00, any player may type |c0077ff77-rdy or -ready|r |c0000ff00to start the game.|r" ))
    call DisplayTimedTextToForce(udg_PlayerGroup_PlyGrpArray[3], 120.00, "TRIGSTR_2589")
    call DestroyForce(udg_PlayerGroup_PlyGrpArray[3])
    call EnableTrigger(gg_trg_Timer)
endfunction

