function Trig_Selling_Towers_Actions takes nothing returns nothing
    if ( Trig_Selling_Towers_Func001C() ) then
        if ( Trig_Selling_Towers_Func001Func001C() ) then
            if ( Trig_Selling_Towers_Func001Func001Func004C() ) then
                set udg_Integer_AuraTowerSell[GetConvertedPlayerId(GetOwningPlayer(GetSpellAbilityUnit()))]=0
                call AdjustPlayerStateBJ(( ( udg_Integer_SellPercent * GetUnitPointValue(GetSpellAbilityUnit()) ) / 100 ), GetOwningPlayer(GetSpellAbilityUnit()), PLAYER_STATE_RESOURCE_GOLD)
                call CreateTextTagUnitBJ(( "Tower Sold For " + ( I2S(( ( udg_Integer_SellPercent * GetUnitPointValue(GetSpellAbilityUnit()) ) / 100 )) + " Gold." ) ), GetSpellAbilityUnit(), 0, 10.00, 0.00, 100, 0.00, 0)
                call RemoveUnit(GetSpellAbilityUnit())
                set udg_Point_PntArray[3]=GetUnitLoc(GetTriggerUnit())
                call AddSpecialEffectLocBJ(udg_Point_PntArray[3], "Abilities\\Spells\\Other\\Transmute\\PileofGold.mdl")
                call RemoveLocation(udg_Point_PntArray[3])
                call DestroyEffectBJ(GetLastCreatedEffectBJ())
            else
                call CreateTextTagUnitBJ("TRIGSTR_131", GetSpellAbilityUnit(), 0, 10.00, 0.00, 100, 0.00, 0)
                set udg_Integer_AuraTowerSell[GetConvertedPlayerId(GetOwningPlayer(GetSpellAbilityUnit()))]=1
                call SetTextTagVelocityBJ(GetLastCreatedTextTag(), 40.00, 90)
                call SetTextTagPermanentBJ(GetLastCreatedTextTag(), false)
                call SetTextTagLifespanBJ(GetLastCreatedTextTag(), 4.00)
                call SetTextTagFadepointBJ(GetLastCreatedTextTag(), 2.00)
                call ShowTextTagForceBJ(false, GetLastCreatedTextTag(), GetPlayersAll())
                call ShowTextTagForceBJ(true, GetLastCreatedTextTag(), GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())))
                call TriggerSleepAction(5.00)
                set udg_Integer_AuraTowerSell[GetConvertedPlayerId(GetOwningPlayer(GetSpellAbilityUnit()))]=0
            endif
        else
            call AdjustPlayerStateBJ(( ( udg_Integer_SellPercent * GetUnitPointValue(GetSpellAbilityUnit()) ) / 100 ), GetOwningPlayer(GetSpellAbilityUnit()), PLAYER_STATE_RESOURCE_GOLD)
            call CreateTextTagUnitBJ(( "Tower Sold For " + ( I2S(( ( udg_Integer_SellPercent * GetUnitPointValue(GetSpellAbilityUnit()) ) / 100 )) + " Gold." ) ), GetSpellAbilityUnit(), 0, 10.00, 0.00, 100, 0.00, 0)
            call RemoveUnit(GetSpellAbilityUnit())
            set udg_Point_PntArray[3]=GetUnitLoc(GetTriggerUnit())
            call AddSpecialEffectLocBJ(udg_Point_PntArray[3], "Abilities\\Spells\\Other\\Transmute\\PileofGold.mdl")
            call RemoveLocation(udg_Point_PntArray[3])
            call DestroyEffectBJ(GetLastCreatedEffectBJ())
        endif
    else
    endif
    call SetTextTagVelocityBJ(GetLastCreatedTextTag(), 40.00, 90)
    call SetTextTagPermanentBJ(GetLastCreatedTextTag(), false)
    call SetTextTagLifespanBJ(GetLastCreatedTextTag(), 4.00)
    call SetTextTagFadepointBJ(GetLastCreatedTextTag(), 2.00)
    call ShowTextTagForceBJ(false, GetLastCreatedTextTag(), GetPlayersAll())
    call ShowTextTagForceBJ(true, GetLastCreatedTextTag(), GetForceOfPlayer(GetOwningPlayer(GetTriggerUnit())))
endfunction

