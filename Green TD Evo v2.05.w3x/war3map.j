            endloop
            call ForForce(udg_PlayerGroup_PlyGrpArray[1], function Trig_Gold_After_Wave_Func001Func006Func005A)
        else
        endif
    else
        call TriggerSleepAction(1.00)
        set udg_Integer_GoldAfterWave=( udg_Integer_GoldAfterWave + GetRandomInt(10, 20) )
        set udg_Integer_TotalPlayerKills=0
        set bj_forLoopAIndex=1
        set bj_forLoopAIndexEnd=9
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            set udg_Integer_TotalPlayerKills=( udg_Integer_TotalPlayerKills + udg_Integer_Array_Kills[GetForLoopIndexA()] )
