function Trig_Generate_Random_Waves_Actions takes nothing returns nothing
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=udg_Integer_WaveAmount
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        set udg_Real_Array_RandomWaveBounty[GetForLoopIndexA()]=( ( ( I2R(GetForLoopIndexA()) + 5.00 ) * udg_Real_CreepHealthMultiplier ) * 20.00 )
        set udg_Integer_Array_RWBounty[GetForLoopIndexA()]=R2I(udg_Real_Array_RandomWaveBounty[GetForLoopIndexA()])
        if ( Trig_Generate_Random_Waves_Func001Func003C() ) then
            set udg_Integer_Array_RWBounty[GetForLoopIndexA()]=( udg_Integer_Array_RWBounty[GetForLoopIndexA()] * 2 )
        else
        endif
        set udg_Real_CreepHealth=( udg_Real_CreepHealth + ( 100.00 * I2R(GetForLoopIndexA()) ) )
        set udg_Real_CreepHealthTotal=( udg_Real_CreepHealth * 20.00 )
        if ( Trig_Generate_Random_Waves_Func001Func006C() ) then
            set udg_Real_Array_HPMultiplierWaves[GetForLoopIndexA()]=GetRandomReal(udg_Real_Array_ChaosHealthPerLow[udg_Integer_ChaosMode], udg_Real_Array_ChaosHealthPerHigh[udg_Integer_ChaosMode])
            set udg_Real_CreepHealthTotal=( udg_Real_CreepHealthTotal * udg_Real_Array_HPMultiplierWaves[GetForLoopIndexA()] )
        else
            set udg_Real_CreepHealthTotal=( udg_Real_CreepHealthTotal * udg_Real_CreepHealthMultiplier )
        endif
        set udg_Integer_Array_WaveSpawnMax[GetForLoopIndexA()]=GetRandomInt(10, 30)
        set udg_Real_Array_RWArmour[GetForLoopIndexA()]=( udg_Real_Array_RWArmour[( GetForLoopIndexA() - 1 )] + I2R(GetForLoopIndexA()) )
        set udg_Real_Array_RWArmourTotal[GetForLoopIndexA()]=( udg_Real_Array_RWArmour[GetForLoopIndexA()] / I2R(udg_Integer_Array_WaveSpawnMax[GetForLoopIndexA()]) )
        set udg_Real_Array_CreepHealthWaves[GetForLoopIndexA()]=( udg_Real_CreepHealthTotal / I2R(udg_Integer_Array_WaveSpawnMax[GetForLoopIndexA()]) )
        if ( Trig_Generate_Random_Waves_Func001Func011C() ) then
            set udg_Integer_Array_WaveSpawnMax[GetForLoopIndexA()]=( udg_Integer_Array_WaveSpawnMax[GetForLoopIndexA()] * 2 )
        else
        endif
        set udg_Integer_Array_RWUnitType[GetForLoopIndexA()]=GetRandomInt(1, 7)
        // Air
        if ( Trig_Generate_Random_Waves_Func001Func014C() ) then
            set udg_UnitType_Array_RWSpawnUnit[GetForLoopIndexA()]=udg_UnitType_Array_RWAir[GetRandomInt(1, 43)]
        else
        endif
        // Hero
        if ( Trig_Generate_Random_Waves_Func001Func016C() ) then
            set udg_UnitType_Array_RWSpawnUnit[GetForLoopIndexA()]=udg_UnitType_Array_RWHero[GetRandomInt(1, 43)]
        else
        endif
        // Immune
        if ( Trig_Generate_Random_Waves_Func001Func018C() ) then
            set udg_UnitType_Array_RWSpawnUnit[GetForLoopIndexA()]=udg_UnitType_Array_RWImmune[GetRandomInt(1, 43)]
        else
        endif
        // Fortified
        if ( Trig_Generate_Random_Waves_Func001Func020C() ) then
            set udg_UnitType_Array_RWSpawnUnit[GetForLoopIndexA()]=udg_UnitType_Array_RWFortified[GetRandomInt(1, 43)]
        else
        endif
        // Heavy
        if ( Trig_Generate_Random_Waves_Func001Func022C() ) then
            set udg_UnitType_Array_RWSpawnUnit[GetForLoopIndexA()]=udg_UnitType_Array_RWHeavy[GetRandomInt(1, 43)]
        else
        endif
        // Light
        if ( Trig_Generate_Random_Waves_Func001Func024C() ) then
            set udg_UnitType_Array_RWSpawnUnit[GetForLoopIndexA()]=udg_UnitType_Array_RWLight[GetRandomInt(1, 43)]
        else
        endif
        // Medium
        if ( Trig_Generate_Random_Waves_Func001Func026C() ) then
            set udg_UnitType_Array_RWSpawnUnit[GetForLoopIndexA()]=udg_UnitType_Array_RWMedium[GetRandomInt(1, 43)]
        else
        endif
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
endfunction

